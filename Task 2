VectorOne=    [1,3,5,6]
VectorTwo=    [2,5,10,12,]
VectorThree=  [3,7,15,18]
VectorFour=   [4,9,20,12]
VectorFive=   [5,11,25,28]
VectorSix=    [6,13,30,36]
VectorSeven=  [7,15,32,37]
VectorEight=  [8,17,34,39]
VectorNine=   [9,19,35,39]
VectorTen=    [14,34,65,49]

A = []
B = []
C = []
D = []
E = []

print('Vector1: ', VectorOne, 'Vector2:', VectorTwo)
print('Inner Product of two vectors: (VectorOne * VectorTwo)')
for i in range(len(VectorOne)):
    product1 = VectorOne[i] * VectorTwo[i]
    print(VectorOne[i], '*', VectorOne[i], '=', product1, '+')
    A.append(product1)
print('(VectorOne * VectorTwo) = ',A)
print('The sum of all numbers :', sum(A))
print('#'*64)
##########################################

print('Vector3: ', VectorThree, 'Vector4:', VectorFour)
print('Inner Product of two vectors: (VectorThree * VectorFour)')
for i in range(len(VectorThree)):
    product2 = VectorThree[i] * VectorFour[i]
    print(VectorThree[i], '*', VectorFour[i], '=', product2, '+')
    B.append(product2)
print('(VectorThree * VectorFour) = ',B)
print('The sum of all numbers :', sum(B))
print('#'*64)
##########################################

