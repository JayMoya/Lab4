print('Vector9: ', VectorNine, 'Vector9:', VectorTen)
print('Inner Product of two vectors: (VectorNine* VectorTen)')
for i in range(len(VectorNine)):
    product5 = VectorNine[i] * VectorTen[i]
    print(VectorNine[i], '*', VectorTen[i], '=', product5, '+')
    E.append(product5)
